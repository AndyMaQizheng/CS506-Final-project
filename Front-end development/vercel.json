{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/api/(.*)",
      "destination": "../not-found-page/404.js"
    },

    {"source": "/home",
      "destination": "../homepage.html"
    },

    {"source": "/account/log in",
      "destination": "../account/login.html"
    },
    {"source": "/account/sign up",
      "destination": "../account/signup.html"
    },
    {"source": "/cart",
      "destination": "../cart/cart.html"
    },
    {"source": "/product",
      "destination": "../cart/product.html"
    },
    {"source": "/checkout",
      "destination": "../payment/payment.html"
    },
    {"source": "/card",
      "destination": "../payment/card.html"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "status": 404, "dest": "../not-found-page/404.html" }
  ]
}
